<%= form_tag '/' do %>
	<%= label_tag :ticker %>
	<%= text_field_tag :ticker %>
	<%= submit_tag :submit %>
<% end %>
<pre>
	<%= params[:ticker] == nil ? 'AAPL' : params[:ticker] %>
	Date: <%= @dataset.date %><br>
	Open: <%= @dataset.open %><br>
	High: <%= @dataset.high %><br>
	Low: <%= @dataset.low %><br>
	Close: <%= @dataset.close %><br>
</pre>